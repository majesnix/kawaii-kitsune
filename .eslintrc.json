{
  "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module",
        "ecmaVersion": 8
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "error",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "semi": ["warn", "always"],
        "eqeqeq": "warn",
        "curly": "off",
        "quotes": ["warn", "single"],
        "arrow-parens": "warn",
        "arrow-spacing": "warn"
    },
    "globals": {
        "USERAGENT": true,
        "moment": true,
        "commandsProcessed": true,
        "cleverbotTimesUsed": true
    }
}